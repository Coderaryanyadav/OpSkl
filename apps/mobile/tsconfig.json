{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "skipLibCheck": true,
        "lib": [
            "esnext"
        ],
        "moduleResolution": "bundler",
        "noEmit": true,
        "baseUrl": "./src",
        "paths": {
            "@/*": [
                "./*"
            ]
        },
        "jsx": "react-native"
    },
    "exclude": [
        "node_modules",
        "babel.config.js",
        "metro.config.js",
        "jest.config.js",
        "ios",
        "android",
        "__tests__",
        ".expo"
    ]
}